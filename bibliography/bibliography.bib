}

@article{vale_influence_2017,
	title = {The influence of the impedance function on gravity-based pedestrian accessibility measures: {A} comparative analysis},
	volume = {44},
	issn = {2399-8083, 2399-8091},
	shorttitle = {The influence of the impedance function on gravity-based pedestrian accessibility measures},
